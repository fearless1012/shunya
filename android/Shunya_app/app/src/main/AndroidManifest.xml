<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ramya.shunya_app" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.ramya.shunya_app.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.ramya.shunya_app.SecActivity"
            android:label="@string/title_activity_sec" >
        </activity>
        <activity
            android:name="com.example.ramya.shunya_app.Forms"
            android:label="@string/title_activity_forms" >
        </activity>
        <activity
            android:name="com.example.ramya.shunya_app.Policies"
            android:label="@string/title_activity_policies" >
        </activity>
        <activity
            android:name="com.example.ramya.shunya_app.Fill"
            android:label="@string/title_activity_fill" >
        </activity>
    </application>

</manifest>
